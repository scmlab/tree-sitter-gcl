{
  "name": "GCL",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_declaration"
          }
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        }
      ]
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "variable_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "constant_declaration"
        }
      ]
    },
    "variable_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "var"
        },
        {
          "type": "SYMBOL",
          "name": "_variable_list"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "constant_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "con"
        },
        {
          "type": "SYMBOL",
          "name": "_constant_list"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "type"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "skip"
        },
        {
          "type": "SYMBOL",
          "name": "abort"
        }
      ]
    },
    "skip": {
      "type": "STRING",
      "value": "skip"
    },
    "abort": {
      "type": "STRING",
      "value": "abort"
    },
    "variable": {
      "type": "PATTERN",
      "value": "[a-z][\\\\_\\\\-\\\\'a-zA-Z0-9]*"
    },
    "constant": {
      "type": "PATTERN",
      "value": "[A-Z][\\\\_\\\\-\\\\'a-zA-Z0-9]*"
    },
    "type": {
      "type": "PATTERN",
      "value": "[A-Z][\\\\_\\\\-\\\\'a-zA-Z0-9]*"
    },
    "_variable_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "variable"
              },
              {
                "type": "STRING",
                "value": ","
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "_constant_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "REPEAT",
          "content": {
            "type": "SEQ",
            "members": [
              {
                "type": "SYMBOL",
                "name": "constant"
              },
              {
                "type": "STRING",
                "value": ","
              }
            ]
          }
        },
        {
          "type": "SYMBOL",
          "name": "constant"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

